<p id="notice"><%= notice %></p>

<h1>Listing Member Of Parliaments</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Electorate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_of_parliaments.each do |member_of_parliament| %>
      <tr>
        <td><%= member_of_parliament.first_name %></td>
        <td><%= member_of_parliament.last_name %></td>
        <td><%= member_of_parliament.email %></td>
        <td><%= member_of_parliament.electorate.name %></td>
        <td><%= link_to 'Show', member_of_parliament %></td>
        <td><%= link_to 'Edit', edit_member_of_parliament_path(member_of_parliament) %></td>
        <td><%= link_to 'Destroy', member_of_parliament, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

